[[servers]]
host = "flts.lan"
port = 389
use_ssl = false
start_tls = false
ssl_skip_verify = false
bind_dn = "{{ grafana_ldap_user }}"
bind_password = '{{ grafana_ldap_password }}'
search_filter = "(sAMAccountName=%s)"
search_base_dns = ["DC=flts,DC=lan"]

[servers.attributes]
name = "givenName"
surname = "sn"
username = "sAMAccountName"
member_of = "memberOf"
email =  "mail"
